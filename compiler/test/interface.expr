package main

// Specific field; method returns that value
struct Husky {
	string kind = "husky"
	
	// TODO: cannot return interface right now
	// Dog dog
}

// Reciever passed by reference; method returns assigned field
func [Husky h] Kind() string {
	return h.kind
}

func main() {
	Husky h = {
		kind: "husky 2"
	}

	Println(h.Kind())
}