package main

interface Dog {
	Breed() string
}

// No specific fields; method returns static string
struct Poodle {}

// Specific field; method returns that value
struct Bulldog {
	string breed = "bulldog"
}

// Specific field; method returns that value
struct Husky {
	string breed = "husky"
}

// Reciever passed by value; method returns static string
func [Poodle p] Breed() string {
	return "poodle"
}

// Reciever passed by value; method returns assigned field
func [Bulldog b] Breed() string {
	return b.breed
}

// Reciever passed by reference; method returns assigned field
func [*Husky h] Breed() string {
	return h.breed
}

func main() {
	Dog d = Poodle{}
	Println("I should be a Poodle:", d.Breed())

	d = Husky{}
	Println("I should be a Husky:", d.Breed())

	d = Bulldog{}
	Println("I should be a Bulldog:", d.Breed())

	d = Bulldog{
		breed = "bulldog_2"
	}
	Println("I should be a SECOND Bulldog:", d.Breed())
}